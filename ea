      var babybison = L.marker([173, 191], {icon : airIcon}).addTo(map);

      var vechileShoppopup = L.DomUtil.create('div', 'popup');
      vechileShoppopup.innerHTML = '<section class="container"><h3>Flying Buffalo Shop</h3><b>Sells a Flying Buffalo for free to Wind Wanderers.';
      var babybisonpopup = L.DomUtil.create('div', 'popup');
      babybisonpopup.innerHTML = '<section class="container" style="height:130px"><div class="one"><h3>Find the Baby Buffalo - Level Requirement: Level 1</h3><b>Find the lost baby bison and return to the quest giver to claim your reward.</div><div class="two">Rewards:[Normal Reward](Reward if in ATLA Roblox Group)<br>[300] Experience<br>[10](20) Pieces';

      vechileShop.bindPopup(vechileShoppopup);
      babybison.bindPopup(babybisonpopup);

      var rlhideoutpopup = L.DomUtil.create('div', 'popup');
      rlhideoutpopup.innerHTML = '<div class = "rlhideoutpopup"><h1>Rogue Hideout</h1> Click here to see the Rogue Hideout up close.</div>';

      $('div.rlhideoutpopup', rlhideoutpopup).on('click', function() {
      		changeMap('https://i.ibb.co/hCYMNG9/Rogue-Hideout.png');
          rlhideoutfc();
      });

      rlhideout.bindPopup(rlhideoutpopup);

    }
    function wlcampfc(){
      var text = document.createElement('p');
      text.innerHTML = '<h3>Bandit Camp</h3><button onclick="changeMap(\'https://i.ibb.co/x3LH96Z/Rock-Realm-City.png\'); bssfc();">Click to go back to the Rock Realm City map.</button><br><button onclick="changeMap(\'map\'); mapfc();">Click to go back to the main map.</button>';
      document.getElementById('helptext').replaceChild(text, document.getElementById('helptext').childNodes[0]);

      var popup = L.popup()
          .setLatLng([94, 485])
          .setContent("This is the Bandit Camp. This is the spawn of Bandits quest when you're tasked to defeat them. This is also the quest...")
          .openOn(map);

    }

    </script>

    
  
