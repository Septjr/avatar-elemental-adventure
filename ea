    // Find the element with the "helptext" ID
var helptextElement = document.getElementById('helptext');

// Check if the element exists before manipulating it
if (helptextElement) {
    // Set the inner HTML to "A:EA"
    helptextElement.innerHTML = "A:EA";
}

  var map = L.map('map', {
    maxZoom: 1,
    minZoom: 1,
    crs: L.CRS.Simple
}).setView([0, 0], 1);

map.setMaxBounds(new L.LatLngBounds([0, 575], [300, 0]));

// Create an image overlay for the original map
var originalMapUrl = 'https://i.ibb.co/jzV3RSt/Interactive-Map.png';
var originalMap = L.imageOverlay(originalMapUrl, [[0, 575], [300, 0]]).addTo(map);

// Initialize the current map as the original map
var currentMap = originalMap;

function changeMap(imageUrl) {
  // Remove the current map from the map
  currentMap.remove();

  if (imageUrl) {
    // Create a new image overlay with the specified URL
    currentMap = L.imageOverlay(imageUrl, [[0, 575], [300, 0]]).addTo(map);
  } else {
    // If no new URL is provided, use the original map
    currentMap = originalMap;
  }

  // You can remove these lines if they are still needed
  $(".leaflet-popup").remove();
  $(".leaflet-interactive").remove();
}

mapfc();

